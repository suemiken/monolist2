<% content_for :title do %>
  <title><%= @user.name %>-monolist2</title>
<% end %>

<% content_for :meta_description do %>
  <meta name="description" content="have:item <% @user.have_items.each do |item| %><%= item.title %> <% end %>want:item<% @user.want_items.each do |item| %><%= item.title %> <% end %>">
<% end %>

<%= render 'profile' , user: @user %>


<%= render 'items/list' , items: @items %>